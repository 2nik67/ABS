<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane id="admin-scroll-pane" fx:id="scrollPaneAdmin" fitToHeight="true" fitToWidth="true" hmax="0.0" pickOnBounds="false" vmax="0.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminbody.AdminController">
   <content>
      <BorderPane id="admin-main-border-pane">
         <center>
            <ScrollPane id="admin-center-scroll-pane" fitToHeight="true" fitToWidth="true" hvalue="1.0" vvalue="1.0" BorderPane.alignment="CENTER">
               <content>
                  <VBox id="admin-vbox" minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <TreeView id="admin-loans-treeview" fx:id="loansTreeView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="113.0" prefWidth="597.0" showRoot="false" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" top="40.0" />
                           </VBox.margin>
                        </TreeView>
                        <TreeView id="admin-clients-treeview" fx:id="clientsTreeView" prefHeight="113.0" prefWidth="597.0" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets bottom="40.0" left="5.0" right="5.0" top="40.0" />
                           </VBox.margin>
                        </TreeView>
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </center>
         <top>
            <BorderPane id="admin-upper-border-pane" BorderPane.alignment="CENTER">
               <center>
                  <HBox id="admin-hbox" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0" BorderPane.alignment="CENTER">
                     <children>
                        <ProgressBar id="admin-progressbar" fx:id="progressBar" prefWidth="200.0" progress="0.0" />
                        <Label id="admin-progress-label" fx:id="progressLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="57.0" text="0%" />
                     </children>
                  </HBox>
               </center>
               <left>
                  <Button id="admin-yaz-button" fx:id="increaseYazBtn" mnemonicParsing="false" onAction="#IncreaseYaz" styleClass="admin-controls" text="Increase YAZ" BorderPane.alignment="CENTER" />
               </left>
               <right>
                  <Button id="admin-load-button" fx:id="loadFileBtn" mnemonicParsing="false" onAction="#ReadFile" styleClass="admin-controls" text="Load File" BorderPane.alignment="CENTER" />
               </right>
            </BorderPane>
         </top>
      </BorderPane>
   </content>
</ScrollPane>
