<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<TabPane nodeOrientation="LEFT_TO_RIGHT" tabClosingPolicy="UNAVAILABLE" tabMaxHeight="1.0E8" tabMaxWidth="1.0E8" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientbody.ClientController">
  <tabs>
      <Tab id="informationTab" fx:id="informationTab" closable="false" text="Information">
         <content>
            <ScrollPane fitToHeight="true" fitToWidth="true">
               <content>
                  <SplitPane dividerPositions="0.5">
                     <items>
                        <VBox minHeight="-Infinity" minWidth="-Infinity">
                           <children>
                              <TreeView fx:id="loansTreeView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="212.0" prefWidth="248.0" VBox.vgrow="SOMETIMES">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </TreeView>
                              <TreeView id="investmentsTreeView" fx:id="investmentsTreeView" prefHeight="212.0" prefWidth="248.0" VBox.vgrow="SOMETIMES">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </TreeView>
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                              <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                                 <children>
                                    <Button id="depositBtn" fx:id="depositBtn" mnemonicParsing="false" text="Deposit">
                                       <HBox.margin>
                                          <Insets right="15.0" top="10.0" />
                                       </HBox.margin></Button>
                                    <Button id="withdrawBtn" fx:id="withdrawBtn" mnemonicParsing="false" text="Withdraw">
                                       <HBox.margin>
                                          <Insets left="15.0" top="10.0" />
                                       </HBox.margin></Button>
                                 </children>
                              </HBox>
                              <TreeView prefHeight="365.0" prefWidth="248.0" VBox.vgrow="SOMETIMES">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </TreeView>
                           </children>
                        </VBox>
                     </items>
                  </SplitPane>
               </content>
            </ScrollPane>
         </content>
      </Tab>
    <Tab id="scrambleTab" fx:id="scrambleTab" text="Scramble" />
    <Tab id="paymentTab" fx:id="paymentTab" text="Payment" />
  </tabs>
</TabPane>
